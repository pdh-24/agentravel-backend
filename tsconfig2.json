{
    "compilerOptions": {
      "strict": true,
      "allowJs": true,
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "jsxImportSource": "hono/jsx",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "outDir": "./build",
      "forceConsistentCasingInFileNames": true,
      "paths": {
        "@/*": ["./*"]
      }
    }
  }